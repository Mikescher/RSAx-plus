<Window x:Class="RSAx_Toolkit.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:RSAx_Toolkit"
		mc:Ignorable="d"
		Title="RSAx-plus Toolkit" Height="650" Width="1000">
	<Grid>
		<TabControl>
			<TabItem Header="Encrypt/Decrypt">
				<local:Page_Crypt />
			</TabItem>
			<TabItem Header="Keygen">
				<local:Page_Keygen />
			</TabItem>
			<TabItem Header="Convert">
				<local:Page_Convert />
			</TabItem>
			<TabItem Header="Checksum">
				<local:Page_Checksum />
			</TabItem>
		</TabControl>
	</Grid>
</Window>
